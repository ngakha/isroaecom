{
  "name": "prshark-ecommerce-server",
  "version": "1.0.0",
  "description": "PRShark Ecommerce Engine - Server",
  "main": "src/core/app.js",
  "scripts": {
    "dev": "nodemon src/core/app.js",
    "start": "node src/core/app.js",
    "migrate": "knex migrate:latest --knexfile config/database.js",
